a=int(input("enter the side of square"))
for x in range(1,a+1):
 if x==1 or x==a:
  for y in range(1,a+1):
   print("1",end=' ')
  print()
 else:
  print("1",end=' ')
  for z in range(2,a):
   print('0',end=' ')
  print("1") 
  
  
